{
  "languageserver": {
    "swift": {
      "enabled": false,
      "command": "/Library/Developer/Toolchains/swift-5.1.3-RELEASE.xctoolchain/usr/bin/sourcekit-lsp",
      "args": [],
      "rootPatterns": ["Package.swift", ".vim/", ".git/"],
      "filetypes": ["c", "swift"]
    },
    "clangd": {
      "enable": false,
      "command": "/usr/local/opt/llvm/bin/clangd",
      "args": ["-completion-style=detailed", "-log=error", "-pretty", "-limit-results=100", "-j=6", "-pch-storage=disk", "-resource-dir=/usr/local/Cellar/llvm/9.0.0_1/lib/clang/9.0.0"],
      "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "ccls": {
      "enable": false,
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  }
}
